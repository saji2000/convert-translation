[{"sura_num": 113, "verse_num": 0, "arabic_text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ", "english_text": "In the name of God, Most Gracious, Most Merciful", "persian_text": "به نام خدا، بخشنده ترين، مهربان ترين.", "subtitle": "", "footnote": "", "chapter_title_english": "Daybreak", "chapter_title_arabic": "الفلق", "chapter_title_persian": "سپیده دم"}, {"sura_num": 113, "verse_num": 1, "arabic_text": "قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ", "english_text": "Say, \"I seek refuge in the Lord of daybreak.", "persian_text": "بگو: \"پناه می برم به پروردگار سپيده دم.", "subtitle": "", "footnote": ""}, {"sura_num": 113, "verse_num": 2, "arabic_text": "مِن شَرِّ مَا خَلَقَ", "english_text": "\"From the evils among His creations.", "persian_text": "\"از شر مخلوقات شرور او.", "subtitle": "", "footnote": ""}, {"sura_num": 113, "verse_num": 3, "arabic_text": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ", "english_text": "\"From the evils of darkness as it falls.", "persian_text": "\"از شر تاريکی چون فرو افتد.", "subtitle": "", "footnote": ""}, {"sura_num": 113, "verse_num": 4, "arabic_text": "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ", "english_text": "\"From the evils of the troublemakers.", "persian_text": "\"از شر مشكل آفرينان.", "subtitle": "", "footnote": ""}, {"sura_num": 113, "verse_num": 5, "arabic_text": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ", "english_text": "\"From the evils of the envious when they envy.\"", "persian_text": "\"از شر حسادت حسودان.\"", "subtitle": "", "footnote": ""}]