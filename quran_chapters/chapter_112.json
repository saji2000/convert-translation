[{"sura_num": 112, "verse_num": 0, "arabic_text": "بِسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ", "english_text": "In the name of God, Most Gracious, Most Merciful", "persian_text": "به نام خدا، بخشنده ترين، مهربان ترين.", "subtitle": "", "footnote": "", "chapter_title_english": "Absoluteness", "chapter_title_arabic": "الإخلاص", "chapter_title_persian": "مطلق"}, {"sura_num": 112, "verse_num": 1, "arabic_text": "قُلْ هُوَ ٱللَّهُ أَحَدٌ", "english_text": "Proclaim, \"He is the One and only GOD.", "persian_text": "اعلام كن: \"اوست يگانه و تنها خدا.", "subtitle": "", "footnote": ""}, {"sura_num": 112, "verse_num": 2, "arabic_text": "ٱللَّهُ ٱلصَّمَدُ", "english_text": "\"The Absolute GOD.", "persian_text": "\"خدای مطلق.", "subtitle": "", "footnote": ""}, {"sura_num": 112, "verse_num": 3, "arabic_text": "لَمْ يَلِدْ وَلَمْ يُولَدْ", "english_text": "\"Never did He beget. Nor was He begotten.", "persian_text": "\"هرگز نزاده و هرگز زاييده نشده است.", "subtitle": "", "footnote": ""}, {"sura_num": 112, "verse_num": 4, "arabic_text": "وَلَمْ يَكُن لَّهُۥ كُفُوًا أَحَد", "english_text": "\"None equals Him.\"", "persian_text": "\"احدی همتای او نيست.\"", "subtitle": "", "footnote": ""}]